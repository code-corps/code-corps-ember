<h4>People <span>{{count}}</span></h4>
<ul>
{{#each (slice 0 15 users) as |user|}}
  <li>
    {{#link-to 'slugged-route' user.username}}
      {{#if user.isLoaded}}
        <img class="icon" src={{user.photoThumbUrl}} />
      {{else}}
        <div class="icon"></div>
      {{/if}}
    {{/link-to}}
  </li>
{{/each}}
</ul>
