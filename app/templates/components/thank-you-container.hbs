<div class="thank-you-container__project-icon" data-test-selector="project icon">
  {{animated-high-five}}
</div>
<h3>Thank you!</h3>
<div class="thank-you-container__message">
  <p data-test-selector="thank you message">
    From all the volunteers on the {{project.title}} team.
  </p>
  <p>
    {{link-to "Back to project" "project" project.organization.slug project.slug}}
  </p>
</div>
<div class="thank-you-container__contributors">
  {{#each contributors as |contributor|}}
    {{volunteer-headshot volunteer=contributor}}
  {{/each}}
</div>
